{{ if_os }}

{% if need_uikit -%}import UIKit{% endif %}
import RxSwift
import RxCocoa


public extension Reactive where Base: {{ cls }} {
{% for name, type in attrs.items() %}
    /// Bindable sink for `{{ name }}` property
    public var {{ name }}: Binder<{{ type }}> {
        return Binder(self.base) { view, attr in
            view.{{ name }} = attr
        }
    }
{% endfor %}
}
#endif
